@page "/manage/labels"
<h3>Labels</h3>
<ul class="nav nav-pills nav-justified">
    <li class="nav-item">
        <a class="nav-link" href="manage/contacts">Contacts</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" href="manage/clients">Clients</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" href="manage/templates">Templates</a>
    </li>
    <li class="nav-item">
        <a class="nav-link active" href="manage/labels">Labels</a>
    </li>

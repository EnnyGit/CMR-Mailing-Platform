<ul>
    @foreach (var label in labels)
    {
        <li class="card" style="min-width: 18rem; max-width:30.5%;">
            <div>
                <h3>@label.Name</h3>
            </div>
        </li>
    }
</ul>
@code {
    List<LableModel> labels = new List<LableModel> { new LableModel { LabelId = 1, Name = "Programmer" },new LableModel { LabelId = 2,Name = "Designer"},new LableModel { LabelId = 3, Name = "Consultant" } };
}
